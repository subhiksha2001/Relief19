            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="match_parent"
                android:layout_height="3500dp"
                app:srcCompat="@drawable/others2" />
            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="match_parent"
                android:layout_height="4000dp"
                app:srcCompat="@drawable/rest" />
            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="match_parent"
                android:layout_height="4500dp"
                app:srcCompat="@drawable/rest1" />
            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="match_parent"
                android:layout_height="5000dp"
                app:srcCompat="@drawable/diet" />
            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="match_parent"
                android:layout_height="5500dp"
                app:srcCompat="@drawable/diet1" />
            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="match_parent"
                android:layout_height="6000dp"
                app:srcCompat="@drawable/hyge" />
            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="match_parent"
                android:layout_height="6500dp"
                app:srcCompat="@drawable/hyge1" />
            <ImageView
                android:id="@+id/imageView15"
                android:layout_width="match_parent"
                android:layout_height="7000dp"
                app:srcCompat="@drawable/expre" />
            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="match_parent"
                android:layout_height="7500dp"
                app:srcCompat="@drawable/expre1" />
            <ImageView
                android:id="@+id/imageView17"
                android:layout_width="match_parent"
                android:layout_height="8000dp"
                app:srcCompat="@drawable/orga" />
            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="match_parent"
                android:layout_height="8500dp"
                app:srcCompat="@drawable/orga1" />
            <ImageView
                android:id="@+id/imageView19"
                android:layout_width="match_parent"
                android:layout_height="9000dp"
                app:srcCompat="@drawable/offer" />
            <ImageView
                android:id="@+id/imageView20"
                android:layout_width="match_parent"
                android:layout_height="9500dp"
                app:srcCompat="@drawable/offer1" />
            <ImageView
                android:id="@+id/imageView21"
                android:layout_width="match_parent"
                android:layout_height="10000dp"
                app:srcCompat="@drawable/evade" />
            <ImageView
                android:id="@+id/imageView22"
                android:layout_width="match_parent"
                android:layout_height="10500dp"
