<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:fillViewport="false">
    <LinearLayout
        android:orientation="vertical" android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/relativeLayout1"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >


            <TextView android:id="@+id/fstTxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="30sp"
                android:textStyle="bold"
                android:textColor="@color/lightBlue500"
                android:layout_marginStart="50dp"
                android:layout_marginTop="37dp"
                android:text="@string/stress_relief_corner"
                android:layout_gravity="center"/>
            <TextView android:id="@+id/fstTxt2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="25sp"
                android:textStyle="bold"
                android:textColor="#070809"
                android:layout_marginStart="100dp"
                android:layout_marginTop="80dp"
                android:text="@string/for_counselling"
                android:layout_gravity="center"/>

            <TextView android:id="@+id/bhoomika"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="25sp"
                android:textStyle="bold"
                android:textColor="#ED105B"
                android:layout_marginStart="30dp"
                android:layout_marginTop="150dp"
                android:text="@string/bhoomika_trust"
                android:layout_gravity="center"/>
            <TextView android:id="@+id/org2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="25sp"
                android:textStyle="bold"
                android:textColor="#ED105B"
                android:layout_marginStart="30dp"
                android:layout_marginTop="200dp"
                android:text="@string/nimhans"
                android:layout_gravity="center"/>
            <TextView android:id="@+id/alliswell"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="35sp"
                android:textStyle="bold"
                android:textColor="#E67C0A"
                android:layout_marginStart="120dp"
                android:layout_marginTop="280dp"
                android:text="@string/all_is_well"
                android:layout_gravity="center"/>


            <Button
                android:id="@+id/buttonCall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#E8CF74"

                android:layout_gravity="center"
                android:layout_marginStart="230dp"
                android:layout_marginTop="150dp"
                android:text="@string/_044_46314726" />
            <Button
                android:id="@+id/buttonCall2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="230dp"
                android:layout_gravity="center"
                android:background="#E8CF74"
                android:layout_marginTop="200dp"
                android:text="@string/_080_46110007" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="match_parent"
                android:layout_height="1000dp"
                app:srcCompat="@drawable/exer" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="match_parent"
                android:layout_height="1500dp"
                app:srcCompat="@drawable/exer1" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="match_parent"
                android:layout_height="2000dp"
